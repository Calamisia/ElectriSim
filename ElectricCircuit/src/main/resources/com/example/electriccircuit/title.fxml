<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox fx:id="masterhbox" prefHeight="700.0" prefWidth="1030.0" spacing="15.0" styleClass="title-background" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.electriccircuit.HelloController">
   <VBox HBox.hgrow="ALWAYS">
      <children>
         <VBox alignment="CENTER" minHeight="0.0" minWidth="0.0" prefHeight="718.0" prefWidth="519.0" spacing="20.0" VBox.vgrow="ALWAYS">
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <Button fx:id="bt1" minHeight="0.0" minWidth="0.0" mnemonicParsing="false" onAction="#LevelSelect" prefHeight="110.0" prefWidth="500.0" styleClass="titlebutton" stylesheets="@styleSheet.css" text="Level selection">
            </Button>
            </HBox>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <Button fx:id="bt2" minHeight="0.0" minWidth="0.0" mnemonicParsing="false" onAction="#Achievements" prefHeight="110.0" prefWidth="500.0" styleClass="titlebutton" text="Achievements" HBox.hgrow="ALWAYS">
            </Button>
            </HBox>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <Button fx:id="bt3" minHeight="0.0" minWidth="0.0" mnemonicParsing="false" onAction="#MainScreen" prefHeight="110.0" prefWidth="500.0" styleClass="titlebutton" text="Sandbox">
            </Button>
            </HBox>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <Button fx:id="bt4" minHeight="0.0" minWidth="0.0" mnemonicParsing="false" onAction="#settings" prefHeight="110.0" prefWidth="500.0" styleClass="titlebutton" text="Settings">
            </Button>
            </HBox>
         </VBox>
      </children>
   </VBox>
   <HBox fx:id="logotitlehbox" alignment="CENTER" minHeight="0.0" minWidth="0.0" prefHeight="699.0" prefWidth="495.0" HBox.hgrow="ALWAYS">
      <VBox alignment="CENTER" minHeight="0.0" minWidth="0.0" prefHeight="704.0" prefWidth="474.0" HBox.hgrow="ALWAYS">
         <HBox alignment="CENTER" minHeight="0.0" minWidth="0.0" VBox.vgrow="ALWAYS">
            <ImageView fx:id="logotitle" fitHeight="400.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
               <Image url="@logo2.jpg" />
            </ImageView>
         </HBox>
      </VBox>
   </HBox>
</HBox>
