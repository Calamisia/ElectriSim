<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="settingsvbox" alignment="TOP_CENTER" prefHeight="400.0" prefWidth="372.0" spacing="15.0" style="-fx-background-color: #808080; -fx-border-color: #000000;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.electriccircuit.HelloController">
   <children>
      <HBox alignment="TOP_CENTER" prefHeight="136.0" prefWidth="360.0">
         <VBox.margin>
            <Insets left="10.0" top="20.0" />
         </VBox.margin>
         <children>
            <Button mnemonicParsing="false" onAction="#exitSettings" prefHeight="25.0" prefWidth="60.0" text="X" HBox.hgrow="NEVER" />
            <HBox alignment="TOP_CENTER" HBox.hgrow="NEVER">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="45.0" prefWidth="250.0" text="Settings" textAlignment="CENTER" HBox.hgrow="NEVER">
                     <font>
                        <Font size="30.0" />
                     </font>
                  </Label>
               </children>
               <HBox.margin>
                  <Insets right="60.0" />
               </HBox.margin>
            </HBox>
         </children>
      </HBox>
      <HBox prefHeight="79.0" prefWidth="350.0" spacing="10.0">
         <children>
            <Label text="Volume">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <Slider blockIncrement="5.0" prefHeight="24.0" prefWidth="257.0" showTickLabels="true" value="50.0" />
         </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="150.0" prefWidth="200.0">
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
         <children>
            <CheckBox mnemonicParsing="false" prefHeight="38.0" prefWidth="224.0" text="CheckBox">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <font>
                  <Font size="26.0" />
               </font>
            </CheckBox>
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#titleScreen" prefHeight="107.0" prefWidth="170.0" text="Exit">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
         <font>
            <Font size="36.0" />
         </font>
      </Button>
   </children>
</VBox>
